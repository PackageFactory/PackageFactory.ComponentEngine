{
    "name": "packagefactory/componentengine",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Wilhelm Behncke",
            "email": "wilhelm.behncke@protonmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "packagefactory/virtualdom": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PackageFactory\\ComponentEngine\\": "src/",
            "PackageFactory\\ComponentEngine\\Test\\": "test/"
        }
    },
    "repositories": {
        "virtual-dom": {
            "url": "git@github.com:PackageFactory/PackageFactory.VirtualDOM.git",
            "type": "vcs"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "phpstan/phpstan": "^1.5",
        "phpunit/php-code-coverage": "^9.2"
    }
}
